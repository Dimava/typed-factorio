{
  "name": "typed-factorio",
  "version": "1.17.0",
  "description": "Featureful typescript definitions for the Factorio modding api.",
  "keywords": [
    "factorio",
    "types",
    "typescript-to-lua",
    "tstl"
  ],
  "repository": "https://github.com/GlassBricks/typed-factorio",
  "license": "MIT",
  "files": [
    "**/*.d.ts",
    "*.d.ts",
    "!generator/**/*"
  ],
  "type": "module",
  "homepage": "https://github.com/GlassBricks/typed-factorio#readme",
  "scripts": {
    "generate": "ts-node --esm -P generator/tsconfig.json generator/main.ts",
    "generate-no-format": "ts-node --swc -T --esm -P generator/tsconfig.json generator/main.ts --no-format",
    "clean": "rimraf runtime/generated",
    "pretest": "npm run generate",
    "test": "jest",
    "lint": "eslint .",
    "check": "npm run lint && npm run test",
    "prepublishOnly": "npm run check",
    "download-latest-json-apis": "ts-node --esm ./scripts/download-latest.ts",
    "new-version-changelog": "ts-node --esm ./scripts/new-version-changelog.ts",
    "next-factorio-version": "npm run download-latest-runtime-api && npm run clean && npm run check && npm run new-version-changelog && npm version minor"
  },
  "peerDependencies": {
    "lua-types": "^2.13.1",
    "typescript-to-lua": "^1.6.1"
  },
  "devDependencies": {
    "@swc/core": "^1.3.78",
    "@types/jest": "^29.5.2",
    "@types/node": "^20.3.1",
    "@types/prettier": "^2.7.3",
    "@typescript-eslint/eslint-plugin": "^5.59.11",
    "@typescript-eslint/parser": "^5.59.11",
    "array.prototype.flatmap": "^1.3.1",
    "chalk": "^5.2.0",
    "eslint": "~8.42.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-import-resolver-typescript": "^3.5.5",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.5.0",
    "lua-types": "^2.13.1",
    "prettier": "^2.8.8",
    "rimraf": "^5.0.1",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "typescript": "~5.1.3",
    "typescript-to-lua": "^1.16.2"
  }
}

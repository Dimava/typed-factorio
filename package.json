{
  "name": "typed-factorio",
  "version": "1.5.1",
  "description": "Featureful typescript definitions for the the Factorio modding lua api.",
  "keywords": [
    "factorio",
    "types",
    "typescript-to-lua",
    "tstl"
  ],
  "repository": "https://github.com/GlassBricks/typed-factorio",
  "license": "MIT",
  "files": [
    "**/*.d.ts"
  ],
  "homepage": "https://github.com/GlassBricks/typed-factorio#readme",
  "scripts": {
    "generate": "ts-node -P generator/tsconfig.json generator/main.ts",
    "clean": "rimraf generated",
    "pretest": "yarn run generate",
    "test": "jest",
    "lint": "eslint .",
    "check": "yarn run lint && yarn run test",
    "prepublishOnly": "yarn run check",
    "download-latest-runtime-api": "ts-node ./scripts/downloadLatest.ts",
    "next-version": "yarn run download-latest-runtime-api && yarn run clean && yarn test && yarn version --minor"
  },
  "peerDependencies": {
    "lua-types": "^2.11.0",
    "typescript-to-lua": "^1.6.1"
  },
  "devDependencies": {
    "@types/jest": "^28.1.6",
    "@types/node": "^18.6.2",
    "@types/prettier": "^2.6.4",
    "@typescript-eslint/eslint-plugin": "^5.31.0",
    "@typescript-eslint/parser": "^5.31.0",
    "chalk": "^4.1.2",
    "eslint": "^8.20.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-import-resolver-typescript": "^3.3.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-n": "^15.2.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.0.0",
    "jest": "^28.1.3",
    "lua-types": "^2.11.0",
    "prettier": "^2.7.1",
    "ts-jest": "^28.0.7",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4",
    "typescript-to-lua": "^1.7.1"
  }
}
